{
  "name": "github-i18n",
  "version": "1.8.164",
  "description": "GitHub中文翻译脚本 - 提供GitHub界面的本地化翻译",
  "main": "GitHub_zh-CN.user.js",
  "scripts": {
    "build": "node build.js",
    "build:major": "node build.js --major",
    "build:minor": "node build.js --minor",
    "build:patch": "node build.js --patch",
    "clean": "node build.js --no-upgrade --no-copy",
    "validate": "node -e \"const BuildManager = require('./build.js'); const bm = new BuildManager(); console.log(bm.validateBuild());\"",
    "test": "npx jest"
  },
  "keywords": [
    "github",
    "i18n",
    "translation",
    "chinese",
    "userscript"
  ],
  "author": "Sut (https://github.com/sutchan)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sutchan/GitHub_i18n.git"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "babel-jest": "^30.2.0",
    "eslint": "^8.57.1",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0"
  },
  "dependencies": {
    "eventsource": "^4.0.0"
  },
  "type": "commonjs"
}
