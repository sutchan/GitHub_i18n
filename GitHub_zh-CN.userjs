            }
        } catch (error) {
            if (CONFIG.debugMode) {
                console.error('[GitHub_i18n] ç‰ˆæœ¬è‡ªåŠ¨å‡çº§å¤„ç†å¤±è´¥:', error);
            }
        }
    }
    
    /**
