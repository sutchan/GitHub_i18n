{
  "source": {
    "include": ["src/", "utils/", "build/"],
    "exclude": ["node_modules/", "dist/", "test/"],
    "includePattern": ".+\\.js$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "opts": {
    "destination": "./docs/build/",
    "recurse": true
  },
  "plugins": [
    "plugins/markdown",
    "plugins/summarize"
  ],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": true
    }
  },
  "markdown": {
    "parser": "gfm",
    "hardwrap": true
  },
  "recurseDepth": 10,
  "sourceType": "module"
}