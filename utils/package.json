{
  "name": "github-auto-string-updater",
  "version": "1.8.17",
  "description": "GitHub字符串自动化更新工具",
  "main": "auto_string_updater.js",
  "scripts": {
    "start": "node auto_string_updater.js",
    "update": "node auto_string_updater.js",
    "server": "node server.js",
    "build:tailwind": "npx tailwindcss -i ./src/tailwind.css -o ./dist/tailwind.css --minify",
    "dictionary:export": "node dictionary_processor.js export",
    "dictionary:import": "node dictionary_processor.js import",
    "dictionary:optimize": "node dictionary_processor.js optimize",
    "dictionary:test": "node test_dictionary_processor.js"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "jsdom": "^22.1.0",
    "morgan": "^1.10.0",
    "node-fetch": "^3.3.2"
  },
  "author": "SutChan",
  "license": "MIT",
  "keywords": [
    "github",
    "i18n",
    "translation",
    "automation"
  ],
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18"
  }
}
